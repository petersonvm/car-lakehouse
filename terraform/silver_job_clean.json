{
    "Role": "arn:aws:iam::901207488135:role/datalake-pipeline-glue-job-role-dev",
    "Command": {
        "Name": "glueetl",
        "ScriptLocation": "s3://datalake-pipeline-glue-scripts-dev/glue_jobs/silver_consolidation_job_clean.py",
        "PythonVersion": "3"
    },
    "DefaultArguments": {
        "--job-bookmark-option": "job-bookmark-disable",
        "--enable-metrics": "true",
        "--enable-spark-ui": "true",
        "--spark-event-logs-path": "s3://datalake-pipeline-glue-logs-dev/sparkHistoryLogs/",
        "--enable-job-insights": "true",
        "--enable-observability-metrics": "true",
        "--bronze_bucket": "datalake-pipeline-bronze-dev",
        "--bronze_json_path": "bronze/car_raw",
        "--silver_bucket": "datalake-pipeline-silver-dev",
        "--silver_path": "car_silver",
        "--database_name": "datalake-pipeline-catalog-dev"
    },
    "Description": "Job Silver limpo com timestamp corrigido para car_silver",
    "MaxRetries": 1,
    "Timeout": 10,
    "GlueVersion": "4.0",
    "NumberOfWorkers": 2,
    "WorkerType": "G.1X"
}